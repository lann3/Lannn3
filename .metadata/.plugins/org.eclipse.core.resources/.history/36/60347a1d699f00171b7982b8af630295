package main;

public class Operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 10;
		int y = 17;
		int sumXy = x + y;
		int subXy = x - y;
		int mulXy = x * y;
		int devXy = y / x;
		int duXy = y % x;
		System.out.println(sumXy);
		System.out.println(subXy);
		System.out.println(mulXy);
		System.out.println(devXy);
		System.out.println(duXy);
		
		float a = 1.23456f;
		float b = 56.54f;
		float sumAb = a + b;
		float sub = b - a;
		float mul = a * b ;
		float dev = a / b;
		float duAb = a % b;
		
		System.out.println("tong hai so =" + sumAb);
		System.out.println("hieu hai so ="+ sub);
		System.out.println("tich hai so =" + mul);
		System.out.println("thuong hai so =" +dev);
		System.out.println("du hai so =" + duAb);
		
		// nang kieu du lieu
		
		byte t = 10;
		short s = 50;
		int i = 100;
		long l = 5000l;
		
		long tongL = l + i + s + t;
		int tongI = i +s + t;
		short tongS = (short) (t + s);
		short tong = (short) (l + i + t + s);
		System.out.println(tongL);
		System.out.println(tongI);
		System.out.println(tongS);
		System.out.println(tong);
		
		int t1 = 10;
		t1 = t1 + 5;
		System.out.println(t1);
		
	}

}
