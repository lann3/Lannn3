package main;

public class Operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 10;
		int y = 17;
		int sumXy = x + y;
		int subXy = x - y;
		int mulXy = x * y;
		float devXy = y / x;
		float duXy = y % x;
		System.out.println(sumXy);
		System.out.println(subXy);
		System.out.println(mulXy);
		System.out.println(devXy);
		System.out.println(duXy);
	}

}
